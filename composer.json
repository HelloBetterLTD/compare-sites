{
  "name": "silverstripers/compare-sites",
  "description": "A tool to help compaing 2 sites.",
  "license": "BSD-2-Clause",
  "authors": [
    {
      "name": "Nivanka Fonseka",
      "email": "nivanka@silverstripers.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "nikic/php-parser": "^3.1",
    "symfony/console": "^3.2 || ^4",
    "symfony/yaml": "^3.2",
    "guzzlehttp/guzzle": "^6.3",
    "symfony/process": "^3.2 || ^4",
    "symfony/filesystem": "^3.2 || ^4",
    "spatie/browsershot": "3.26.2",
    "symfony/dom-crawler": "v4.2.4",
    "symfony/css-selector": "v4.2.4",
    "vajiral/php-image-compare": "1.0.1",
    "twig/twig": "v2.6.2"
  },
  "bin": [
    "bin/compare-sites"
  ],
  "require-dev": {
    "phpunit/phpunit": "^7",
    "mikey179/vfsstream": "^1.6",
    "secondtruth/phar-compiler": "^1.1",
    "roave/security-advisories": "dev-master"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.3.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "SilverStripers\\CompareSites\\": "src/"
    }
  },
  "scripts": {
    "lint": "phpcs src/ tests/",
    "lint-clean": "phpcbf src/ tests/"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}